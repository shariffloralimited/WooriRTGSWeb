                                <span class="alert-icon"><i class="fa fa-bolt"></i></span>
                                <div class="noti-info">
                                    <a href="BranchMessages.aspx"><%# Eval("MessageText") %></a>
                                </div>
                            </div>
                        </li>
                    </ItemTemplate>
                </asp:DataList>
            </ul>
        </li>
        <li>
            <h4>Real Time Gross Settlement</h4>
        </li>
        <!-- notification dropdown end -->
    </ul>
