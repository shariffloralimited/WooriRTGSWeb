        <li class="dropdown">
            <a href="selectrole.aspx">
                <i class="fa fa-tasks"></i>
                <span class="badge bg-success"></span>
            </a>            
        </li>
        <!-- settings end -->
<%--        <!-- inbox dropdown start-->
        <li id="header_inbox_bar" class="dropdown">
